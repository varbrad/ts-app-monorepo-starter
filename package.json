{
  "name": "react-ts-starter",
  "private": true,
  "license": "MIT",
  "scripts": {
    "client:build": "parcel build src/client/index.tsx",
    "client:lint": "eslint src/client --ext .ts,.tsx",
    "client:start": "parcel src/client/index.tsx",
    "client:test": "cross-env JEST_PROJECT=client jest",
    "client:tsc": "tsc src/client --noEmit",

    "server:build": "parcel build src/server/index.tsx",
    "server:lint": "eslint src/server --ext .ts",
    "server:start": "parcel src/server/index.tsx",
    "server:test": "cross-env JEST_PROJECT=server jest",
    "server:tsc": "tsc src/server --noEmit",

    "build": "run-p client:build server:build",
    "lint": "run-p client:lint server:lint",
    "start": "run-p client:start server:start",
    "test": "jest",
    "tsc": "run-p client:tsc server:tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/dom": "^5.2.0",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.14",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
